#include <iostream>
using namespace std;

int main(){
        int n;
            cin >> n;
                int cem = 0;

                    for(int i = 1; i <= n; i++){
                                if(i % 2 == 0){
                                                cem += i * i;
                                }
                    }

                        cout << cem;
}


#include <iostream>
using namespace std;

int main(){
        int a;
            cin >> a;
                int say = 0;

                    while(a > 0){
                                say++;
                                        a /= 10;
                    }

                        cout << say;
}


#include <iostream>
using namespace std;

int main(){
        int N;
            cin >> N;

                for(int i = 2; i < N; i++){
                            bool sadedir = true;

                                    for(int j = 2; j * j <= i; j++){
                                                    if(i % j == 0){
                                                                        sadedir = false;
                                                                                        break;
                                                    }
                                    }

                                            if(sadedir){
                                                            cout << i << " ";
                                            }
                }
}


#include <iostream>
using namespace std;

int main(){
        for(int i = 100; i <= 999; i++){
                    int a = i / 100;
                            int b = (i / 10) % 10;
                                    int c = i % 10;

                                            if(a + b + c == 10){
                                                            cout << i << " ";
                                            }
        }
}


#include <iostream>
using namespace std;

int main(){
        int x;
            cin >> x;
                int say = 0;

                    for(int i = 1; i <= x; i++){
                                if(x % i == 0){
                                                say++;
                                }
                    }

                        cout << say;
}


#include <iostream>
using namespace std;

int main(){
        int N;
            cin >> N;

                int x;
                    int maxN = -999999, minN = 999999;
                        double cem = 0;

                            for(int i = 0; i < N; i++){
                                        cin >> x;
                                                cem += x;
                                                        if(x > maxN) maxN = x;
                                                                if(x < minN) minN = x;
                            }

                                cout << "Min: " << minN << endl;
                                    cout << "Max: " << maxN << endl;
                                        cout << "Orta: " << cem / N;
}


#include <iostream>
using namespace std;

int main(){
        for(int i = 100; i <= 999; i++){
                    int a = i / 100;
                            int b = (i / 10) % 10;
                                    int c = i % 10;

                                            int toplam = a*a*a + b*b*b + c*c*c;

                                                    if(toplam == i){
                                                                    cout << i << " ";
                                                    }
        }
}


#include <iostream>
using namespace std;

int main(){
        int x;
            cin >> x;
                string s = "";

                    while(x > 0){
                                s = char('0' + x % 2) + s;
                                        x /= 2;
                    }

                        cout << s;
}


#include <iostream>
using namespace std;

int main(){
        for(int i = 1; i <= 1000; i++){
                    int t = i, ters = 0;

                            while(t > 0){
                                            ters = ters * 10 + (t % 10);
                                                        t /= 10;
                            }

                                    if(ters == i){
                                                    cout << i << " ";
                                    }
        }
}


#include <iostream>
using namespace std;

int main(){
        double netice, eded;
            char op;

                cin >> netice;

                    while(cin >> op >> eded){
                                if(op == '+') netice += eded;
                                        else if(op == '-') netice -= eded;
                                                else if(op == '*') netice *= eded;
                                                        else if(op == '/') netice /= eded;
                    }

                        cout << netice;
}


#include <iostream>
using namespace std;

int main(){
        int a = 1, b = 1;

            while(a < 1000){
                        cout << a << " ";
                                int c = a + b;
                                        a = b;
                                                b = c;
            }
}


#include <iostream>
using namespace std;

int main(){
        int x;
            cin >> x;
                int sum = 0;

                    for(int i = 1; i < x; i++){
                                if(x % i == 0){
                                                sum += i;
                                }
                    }

                        if(sum == x) cout << "Mükəmməldir";
                            else cout << "Deyil";
}


#include <iostream>
using namespace std;

int main(){
        int n, x;
            cin >> n;

                int tek = 0, cut = 0;

                    for(int i = 0; i < n; i++){
                                cin >> x;
                                        if(i % 2 == 0) cut += x;
                                                else tek += x;
                    }

                        cout << "Cüt indeks: " << cut << endl;
                            cout << "Tək indeks: " << tek;
}


#include <iostream>
using namespace std;

int main(){
        int x;
            cin >> x;

                int t = x, r = 0;

                    while(t > 0){
                                r = r * 10 + (t % 10);
                                        t /= 10;
                    }

                        if(r == x) cout << "Eynidir";
                            else cout << "Fərqlidir";
}


#include <iostream>
using namespace std;

int main(){
        int x;
            int mx = -999999;

                while(true){
                            cin >> x;
                                    if(x == 0) break;
                                            if(x > mx) mx = x;
                }

                    cout << mx;
}
              

                    

                    

                 
                    

            

                    


                                    
                            
        

                    

                                                    
        

                            

                                
                    

                                            
        

                                            
                                                    
                                    
                

                    

                                
                    
